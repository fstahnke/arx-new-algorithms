<?xml version="1.0"?>
<config>
	<!-- add datasets -->
    <dataset>ADULT</dataset>

	<!-- add algorithms -->
    <algorithm>RECURSIVE_GLOBAL_RECODING</algorithm>
    <algorithm>FLASH</algorithm>
    
	<!-- add privacy models -->
    <privacyModel>K5_ANONYMITY</privacyModel>

	<!-- add utility measures -->
    <utilityMeasure>LOSS</utilityMeasure>

	<!-- add suppression limits. can be any double value between 0 and 1 -->
    <suppressionLimit>0.0</suppressionLimit>
    <suppressionLimit>0.01</suppressionLimit>
    <suppressionLimit>0.02</suppressionLimit>
    <suppressionLimit>0.03</suppressionLimit>
    <suppressionLimit>0.05</suppressionLimit>
    <suppressionLimit>0.07</suppressionLimit>
    <suppressionLimit>0.1</suppressionLimit>
    <suppressionLimit>0.15</suppressionLimit>
    <suppressionLimit>0.2</suppressionLimit>
    <suppressionLimit>0.25</suppressionLimit>
    <suppressionLimit>0.3</suppressionLimit>
    <suppressionLimit>0.35</suppressionLimit>
    <suppressionLimit>0.4</suppressionLimit>
    <suppressionLimit>0.45</suppressionLimit>
    <suppressionLimit>0.5</suppressionLimit>
    <suppressionLimit>0.55</suppressionLimit>
    <suppressionLimit>0.6</suppressionLimit>
    <suppressionLimit>0.65</suppressionLimit>
    <suppressionLimit>0.7</suppressionLimit>
    <suppressionLimit>0.75</suppressionLimit>
    <suppressionLimit>0.8</suppressionLimit>
    <suppressionLimit>0.85</suppressionLimit>
    <suppressionLimit>0.9</suppressionLimit>
    <suppressionLimit>0.95</suppressionLimit>
    <suppressionLimit>0.999</suppressionLimit>

	<!-- add gsFactors. Can be any double value between 0 and 1. Closer to 0 
		means more suppression. -->
	<gsFactor>0.05</gsFactor>
	
	<!-- add gsStepSizes -->
	<gsStepSize>0.0</gsStepSize>

	<!-- set paths for output files -->
    <outputFile>results/RGRFlash-suppressionScaling.csv</outputFile>
    <plotFile>results/RGRFlash-suppressionScaling.pdf</plotFile>
    
    <!-- set number of runs -->
    <numberOfRuns>5</numberOfRuns>
</config>